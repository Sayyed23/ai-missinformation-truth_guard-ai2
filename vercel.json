{
  "version": 2,
  "builds": [
    {
      "src": "backend/antigravity/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "truth_guard_ai/build/web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/antigravity/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "truth_guard_ai/build/web/$1"
    }
  ]
}
